import java.io.*;
import java.util.*;
class find {
	public static void main(String[] args)
	{
		int n = 4;
        char o='d';
		String names[]= { "Rahul", "Ajay", "Gourav", "Riya" };
		Arrays.sort(names);
		if(o=='a')
		{
		System.out.println("The names in alphabetical order are: ");
		     for (int i = 0; i < n; i++){
			    System.out.println(names[i]);}
		    
		}
		else if(o=='d')
		{
		    for (int i =n-1; i>=0; i--) {
			System.out.println(names[i]);}
		}
	}
}
